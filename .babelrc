{
  "presets": [
    ["@babel/preset-env", {"modules": false}],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    },
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    }
  }
}